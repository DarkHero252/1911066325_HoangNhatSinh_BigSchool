@model _1911066325_HoangNhatSinh_BigSchool.ViewModels.FollowingViewModel
@{
    ViewBag.Title = "Following Me";
}
<h2>Những người đã follow bạn</h2>
<div class="row">
    <ul class="course">
        @foreach (var follow in Model.Followings)
        {
            <li>
                <div class="details">
                    <span class="lecturer">
                        @follow.Name
                    </span>
                </div>
            </li>
        }
    </ul>
</div>